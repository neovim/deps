#!/bin/sh

exec '/opt/neovim-deps/osx-x64/usr/bin/luajit' -e 'package.path="/opt/neovim-deps/osx-x64/usr/share/lua/5.1/?.lua;/opt/neovim-deps/osx-x64/usr/share/lua/5.1/?/init.lua;"..package.path; package.cpath="/opt/neovim-deps/osx-x64/usr/lib/lua/5.1/?.so;"..package.cpath' -e 'local k,l,_=pcall(require,"luarocks.loader") _=k and l.add_context("luarocks","2.2.0-1")' '/opt/neovim-deps/osx-x64/usr/lib/luarocks/rocks/luarocks/2.2.0-1/bin/luarocks' "$@"
